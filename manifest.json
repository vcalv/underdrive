{
  "manifest_version": 3,
  "name": "Underdrive",
  "version": "1.0",
  "description": "Add download mp3/odm button to overdrive pages",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["*://*.overdrive.com/account/loans"]
    }
  ]
}
